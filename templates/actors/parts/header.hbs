<header class="sheet-header">
    <div class="tab-div">
        <img class="img-logo" src="systems/pokeymanz/assets/icons/pokeball.svg" alt="pokeball icon">
        <input class="charname" name="name" type="text" value="{{actor.name}}" placeholder="{{localize 'Name'}}"
            data-tooltip="{{actor.name}}" />
    </div>
    <nav class="sheet-tabs tabs" aria-role="{{localize 'SHEETS.FormNavLabel'}}">
        {{#each tabs as |tab|}}
        <a class="tab-icon-button {{tab.cssClass}}" data-action="tab" data-group="{{tab.group}}" data-tab="{{tab.id}}">
            <i class="{{tab.icon}}"></i>
        </a>
        {{/each}}
    </nav>
</header>