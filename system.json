{
  "id": "pokeymanz",
  "title": "Pokeymanz TTRPG",
  "description": "A FoundryVTT system for PokeymanzTTRPG, pokemon fan-made tabletop RPG",
  "version": "#{VERSION}#",
  "compatibility": {
    "minimum": "12",
    "verified": "12"
  },
  "authors": [
    {
      "name": "JoaquinP",
      "url": "https://github.com/joaquinpereyra98",
      "email": "joaquinpereyra98@gmail.com",
      "discord": "joaquinp98"
    }
  ],
  "esmodules": [
    "pokeymanz.mjs",
    "greensock/esm/all.js"
  ],
  "styles": [
    "styles/pokeymanz.css"
  ],
  "packs": [],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "grid": {
    "distance": 5,
    "units": "ft"
  },
  "media": [
    {
      "type": "setup",
      "url": "systems/pokeymanz/assets/logo/HighResLogo_1.webp",
      "thumbnail": "systems/pokeymanz/assets/logo/HighResLogo_1.webp"
    }
  ],
  "flags": {
    "hotReload": {
      "extensions": [
        "css",
        "hbs",
        "json"
      ]
    }
  },
  "documentTypes": {
    "Actor": {
      "trainer": {
        "htmlFields": [
          "description.value",
          "description.gmNotes"
        ]
      },
      "pokemon": {
        "htmlFields": [
          "description.value",
          "description.gmNotes",
          "stats.ability"
        ]
      }
    },
    "Item": {
      "feat": {
        "htmlFields": [
          "description.value",
          "description.gmNotes"
        ]
      },
      "gear": {
        "htmlFields": [
          "description.value",
          "description.gmNotes"
        ]
      },
      "move": {
        "htmlFields": [
          "description.value",
          "description.gmNotes"
        ]
      }
    }
  },
  "url": "#{URL}#",
  "manifest": "",
  "download": "#{DOWNLOAD}#"
}